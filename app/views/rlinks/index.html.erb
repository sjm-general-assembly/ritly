<h2>Active Links</h2>
<table class="table table-bordered">
	<tr>
		<th>ID</th>
		<th>URL</th>
		<th>Link String</th>
		<th>Created</th>
		<th>Updated</th>
	</tr>
	<% @rlinks.each do |rlink| %>
		<tr>
			<td><%= rlink[:id]%></td>
			<td><%= rlink[:link]%></td>
			<td><%= rlink[:random_string]%></td>
			<td><%= rlink[:created_at]%></td>
			<td><%= rlink[:updated_at]%></td>
		</tr>
	<% end %>
</table>
